<mvc:View controllerName="com.sce.epportal.enterpriseprocurementportal.controller.RequestTypeDocuments"
   xmlns:table="sap.ui.table"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:u="sap.ui.unified"
    xmlns:c="sap.ui.core"
    xmlns:m="sap.m"
    height="100%">
   
        <mvc:content>
            <table:Table
                class="RequestTable"
                id="scopeItemsTableA"
                selectionMode="None"
                rows="{JSONModel>/requestTypes}"
                ariaLabelledBy="title"
                  rowActionCount="30"
               
            >
                <table:columns>
                    <table:Column width="14rem">
                        <m:Label text="Request Type" />
                        <table:template>
                            <m:Text text="{JSONModel>name}" wrapping="false" />
                        </table:template>
                    </table:Column>
 
                    <table:Column width="8rem">
                        <m:Label text="SOW" vAlign="Middle" />
                        <table:template>
                            <m:CheckBox
                                selected="{
                                    path: 'Heavy',
                                    type: 'sap.ui.model.type.String'
                                }"
                            />
                        </table:template>
                    </table:Column>
 
                    <table:Column width="8rem">
                        <m:Label text="JOA" />
                        <table:template>
                            <m:CheckBox
                                selected="{
                                    path: 'Heavy',
                                    type: 'sap.ui.model.type.String'
                                }"
                            />
                        </table:template>
                    </table:Column>
 
                    <table:Column width="8rem">
                        <m:Label text="CYBER RISK" />
                        <table:template>
                            <m:CheckBox
                                selected="{
                                    path: 'Heavy',
                                    type: 'sap.ui.model.type.String'
                                }"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column width="8rem">
                        <m:Label text="NDA" />
                        <table:template>
                            <m:CheckBox
                                selected="{
                                    path: 'Heavy',
                                    type: 'sap.ui.model.type.String'
                                }"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column width="8rem">
                        <m:Label text="POC" />
                        <table:template>
                            <m:CheckBox
                                selected="{
                                    path: 'Heavy',
                                    type: 'sap.ui.model.type.String'
                                }"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column width="8rem">
                        <m:Label text="MA" />
                        <table:template>
                            <m:CheckBox
                                selected="{
                                    path: 'Heavy',
                                    type: 'sap.ui.model.type.String'
                                }"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column width="8rem">
                        <m:Label text="Chagne Order" />
                        <table:template>
                            <m:CheckBox
                                selected="{
                                    path: 'Heavy',
                                    type: 'sap.ui.model.type.String'
                                }"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column width="8rem">
                        <m:Label text="Pur.Req." />
                        <table:template>
                            <m:CheckBox
                                selected="{
                                    path: 'Heavy',
                                    type: 'sap.ui.model.type.String'
                                }"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column width="8rem">
                        <m:Label text="Check List" />
                        <table:template>
                            <m:CheckBox
                                selected="{
                                    path: 'Heavy',
                                    type: 'sap.ui.model.type.String'
                                }"
                            />
                        </table:template>
                    </table:Column>
 
                     <table:Column width="8rem">
                        <m:Label text="Other" />
                        <table:template>
                            <m:CheckBox
                                selected="{
                                    path: 'Heavy',
                                    type: 'sap.ui.model.type.String'
                                }"
                            />
                        </table:template>
                    </table:Column>
                </table:columns>
            </table:Table>
        </mvc:content>
    
</mvc:View>
